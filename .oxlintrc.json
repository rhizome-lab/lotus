{
  "plugins": [
    "typescript",
    "import",
    "unicorn",
    "react",
    "react-hooks",
    "jest"
  ],
  "rules": {
    "no-console": "off",
    "no-debugger": "warn",
    "eqeqeq": "warn"
  },
  "overrides": [
    {
      "files": ["*.test.ts", "*.test.tsx", "*.spec.ts", "*.spec.tsx"],
      "rules": {
        "no-magic-numbers": "off"
      }
    }
  ]
}
